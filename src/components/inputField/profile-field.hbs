<div class="test">
    <div class="field-profile">
        <label
                class="field-profile__label"
                for={{ name }}
        >
            {{ label }}
        </label>
        {{{
        Input
                type=type
                name=name
                placeholder=placeholder
                disabled=disabled
                env=env
                onBlur=onBlur
                ref="input"
        }}}
    </div>
    <p class="field-profile__error">
        {{{ ErrorText error=error ref="errorText" }}}
    </p>
</div>



